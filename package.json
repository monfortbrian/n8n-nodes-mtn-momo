{
  "name": "n8n-nodes-mtn-momo",
  "version": "1.0.0",
  "description": "n8n node for MTN Mobile Money API - Collections and Disbursements",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "node -e \"require('fs').copyFileSync('src/nodes/MtnMomo/mtnmomo.svg', 'dist/nodes/MtnMomo/mtnmomo.svg')\"",
    "dev": "tsc --watch",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "n8n",
    "n8n-nodes",
    "mtn-momo",
    "mobile-money",
    "payments",
    "africa",
    "fintech"
  ],
  "author": "Monfort Brian <monfortnkurunziza0@gmail.com>",
  "license": "MIT",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MtnMomoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MtnMomo/MtnMomo.node.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monfortbrian/n8n-nodes-mtn-momo.git"
  },
  "bugs": {
    "url": "https://github.com/monfortbrian/n8n-nodes-mtn-momo/issues"
  },
  "homepage": "https://github.com/monfortbrian/n8n-nodes-mtn-momo#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "n8n-workflow": "^2.7.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "dotenv": "^17.2.3",
    "uuid": "^13.0.0"
  }
}
